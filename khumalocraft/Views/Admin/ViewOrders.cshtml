@model IEnumerable<khumalocraft.Models.Order>

@{
    ViewData["Title"] = "View Orders";
}

<h1>View Orders</h1>

<table class="table">
    <thead>
        <tr>
            <th>Order Date</th>
            <th>Total Amount</th>
            <!-- Add other headers if needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderDate</td>
                <td>@order.TotalAmount</td>
                <!-- Add other order details -->
            </tr>
        }
    </tbody>
</table>